{
    "name": "connectholland/secure-jwt",
    "type": "library",
    "description": "Library to increate security of JWT based security",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "lexik/jwt-authentication-bundle": "^2.6",
        "ext-json": "*",
        "doctrine/orm": "^2.7"
    },
    "autoload": {

{
    "name": "connectholland/secure-jwt",
    "type": "library",
    "description": "Library to increate security of JWT based security",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "lexik/jwt-authentication-bundle": "^2.6",
        "ext-json": "*",
        "doctrine/orm": "^2.7",
        "symfony/messenger": "^5.0",
        "api-platform/api-pack": "^1.2",
        "api-platform/core": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "ConnectHolland\\SecureJWT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConnectHolland\\SecureJWT\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    }
}
